import re

# Assume the OCR output from the previous step
text = """
Best Before: 12/31/2024
Manufactured: 01/01/2023
"""

# Regular expression to extract expiry date in MM/DD/YYYY or similar formats
expiry_date_pattern = r'(\d{2}/\d{2}/\d{4})'

expiry_date = re.findall(expiry_date_pattern, text)

if expiry_date:
    print(f"Expiry Date: {expiry_date[0]}")
else:
    print("Expiry date not found")
